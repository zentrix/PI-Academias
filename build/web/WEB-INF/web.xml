<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Sindex</servlet-name>
        <servlet-class>Servlet.SIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SLogin</servlet-name>
        <servlet-class>Servlet.SLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SListas</servlet-name>
        <servlet-class>Servlet.SLista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sminuta</servlet-name>
        <servlet-class>Servlet.minutaservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SReuniones</servlet-name>
        <servlet-class>Servlet.reunionesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sindex</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SListas</servlet-name>
        <url-pattern>/listas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sminuta</servlet-name>
        <url-pattern>/minuta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SReuniones</servlet-name>
        <url-pattern>/reunion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>/login</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>404</error-code>
        <location>/404.html</location>
    </error-page>
</web-app>
